class TestClass {
    constructor() {
      test()
    }

    constructor(x: int, y: int) {
      print sum(x, y)
    }

    fun test() {
        print sum(1, 2)
    }

    fun sum(x: int, y: int): int {
        return x + y
    }
}

new TestClass()

new TestClass(1, 1)

class TestDefaultConstructor {

    fun test() {
        print 'test'
    }

}

var c = new TestDefaultConstructor()
c.test()
